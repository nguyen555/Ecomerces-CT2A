    function setlocalStorage(name, val) {
        if (localStorage !== undefined) {
            localStorage.setItem(name, val);
        }
    }

    function getlocalStorage(name) {
        if (localStorage !== undefined) {
            var data = localStorage.getItem(name);
            if (data !== null)
                return data;
        }
        return null;
    }

    function CheckApprise() {
