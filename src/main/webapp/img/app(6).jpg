        <script type="text/javascript">
            var isMobile = 0;
            var isUpdateProvinceName = 0;
            function OpenProvince($this) {
                $('.h-province>div>b').not($this).removeClass('active');
                $('.phukien').removeClass('actmenu');
                $('.phukien').next().slideUp(100);
                $('.h-province>div>div').not($this.next()).hide();
                $this.next().toggle();
                $this.toggleClass('active');
                $('.menu').toggleClass('index');

                if ($this.parent().parent().hasClass('detail')) return;

                $('.h-overlay').toggle();
            }
            function SearchProvince(ele) {
                var $this = $(ele);
                var term = $this.val();
                $this.parent().next().find('a').each(function () {
                    var v = $(this).text();
                    var rEscape = /[\-\[\]{}()*+?.,\\\^$|#\s]/g;
                    var regex = new RegExp(term.replace(rEscape, "\\$&"), 'gi');
                    var regex2 = new RegExp(RemoveVietNamChar(term).replace(rEscape, "\\$&"), 'gi');
                    if (v.search(regex) !== -1 || RemoveVietNamChar(v).search(regex) !== -1 || RemoveVietNamChar(v).search(regex2) !== -1) {
                        $(this).show();
                    } else {
                        $(this).hide();
                    }
                });
            }
            function RemoveVietNamChar(str) {
                str = str.replace(/à|á|ạ|ả|ã|â|ầ|ấ|ậ|ẩ|ẫ|ă|ằ|ắ|ặ|ẳ|ẵ/g, "a");
                str = str.replace(/è|é|ẹ|ẻ|ẽ|ê|ề|ế|ệ|ể|ễ/g, "e");
                str = str.replace(/ì|í|ị|ỉ|ĩ/g, "i");
                str = str.replace(/ò|ó|ọ|ỏ|õ|ô|ồ|ố|ộ|ổ|ỗ|ơ|ờ|ớ|ợ|ở|ỡ/g, "o");
                str = str.replace(/ù|ú|ụ|ủ|ũ|ư|ừ|ứ|ự|ử|ữ/g, "u");
                str = str.replace(/ỳ|ý|ỵ|ỷ|ỹ/g, "y");
                str = str.replace(/đ/g, "d");

                str = str.replace(/À|Á|Ạ|Ả|Ã|Â|Ầ|Ấ|Ậ|Ẩ|Ẫ|Ă|Ằ|Ắ|Ặ|Ẳ|Ẵ/g, "A");
                str = str.replace(/È|É|Ẹ|Ẻ|Ẽ|Ê|Ề|Ế|Ệ|Ể|Ễ/g, "E");
                str = str.replace(/Ì|Í|Ị|Ỉ|Ĩ/g, "I");
                str = str.replace(/Ò|Ó|Ọ|Ỏ|Õ|Ô|Ồ|Ố|Ộ|Ổ|Ỗ|Ơ|Ờ|Ớ|Ợ|Ở|Ỡ/g, "O");
                str = str.replace(/Ù|Ú|Ụ|Ủ|Ũ|Ư|Ừ|Ứ|Ự|Ử|Ữ/g, "U");
                str = str.replace(/Ỳ|Ý|Ỵ|Ỷ|Ỹ/g, "Y");
                str = str.replace(/Đ/g, "D");

                return str;
            }
            function ChangeProvince(province) {
                $('.h-loading').removeClass('hide');
                if (isMobile == 0) $('.h-province>div>div').hide();

                var data = new FormData();
                data.append('province', province);

                var xhttpProvince = new XMLHttpRequest();
                xhttpProvince.onreadystatechange = function () {
                    if (this.readyState == 4 && this.status == 200) {
                        window.location.reload();
                    }
                };

                xhttpProvince.open("POST", "/aj/CommonV3/UpdateProvince", true);
                xhttpProvince.send(data);
            }
            function UpdateProvinceName() {
                var xhttpProvince = new XMLHttpRequest();
                xhttpProvince.onreadystatechange = function () { 
                    if (this.readyState == 4 && this.status == 200) {
                        var res = JSON.parse(this.responseText);
                        if (res != undefined) {
                            document.getElementById('h-provincename').innerHTML = res.provinceName; 
                            document.getElementById('h-provincename').setAttribute('data-provinceid', res.provinceId); 
                        }
                    }
                }; 
                xhttpProvince.open('GET', '/aj/CommonV3/GetProvinceName', true);
                xhttpProvince.send();
            }
            if (isUpdateProvinceName == 1) {
                UpdateProvinceName();
            }

            var path = location.pathname;
            if ((path.indexOf('/tra-gop') >= 0 && path.indexOf('/dat-mua') >= 0)) {
                var $element = document.getElementById('h-provincename');
                $element.removeAttribute('onclick');
                $element.classList.add('gray');
            }
        </script>


