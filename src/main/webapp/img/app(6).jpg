        <script type="text/javascript">
            var isNewCart2020 = (getCookie("cartnew") !== undefined && getCookie("cartnew") === "1") ? 1 : 0;
            var path = document.location.pathname;
            var paths = ['/tin-tuc', '/game-app', '/hoi-dap', '/may-doi-tra', '/sim-so-dep', '/tien-ich', '/gio-hang'];
            var items = ['mn-new', 'mn-gameapp', 'mn-faq', 'mn-maydoitra', 'mn-sim', 'mn-utility', 'a1'];
            for (var i = 0; i < paths.length; i++) {
                if (path.indexOf(paths[i]) > -1) {
                    var element = document.getElementsByClassName(items[i]);
                    element[0].classList.add('actmenu');
                }
            }

            function getCookie(c_name) {
                var i, x, y, ARRcookies = document.cookie.split(";");
                for (i = 0; i < ARRcookies.length; i++) {
                    x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
                    y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
                    x = x.replace(/^\s+|\s+$/g, "");
                    if (x == c_name) {
                        return unescape(y);
                    }
                }
            }

            function UpdateCartQuantity() {
                if (isNewCart2020 == 0) return;

                var xhttpCartNew = new XMLHttpRequest();
                xhttpCartNew.onreadystatechange = function () {
                    if (this.readyState == 4 && this.status == 200) {
                        var res = JSON.parse(this.responseText);
                        if (res != undefined) {
                            var e1 = document.getElementsByClassName('a1')[0];
                            var cart = document.getElementsByClassName('icontgdd-cart')[0];
                            if (res.items_count != undefined && parseInt(res.items_count) > 0) {
                                e1.classList.add('actmenu');
                                cart.innerText = res.items_count;
                            } else {
                                e1.classList.remove('actmenu');
                                cart.innerText = '';
                            }
                        }
                    }
                };
                xhttpCartNew.open("GET", "/cart/api/cart/info", true);
                xhttpCartNew.send();
            }

            function LoadPK() {
                if ($('.h-province>div>b').hasClass('active')) {
                    $('.h-province>div>b').removeClass('active');
                    $('.h-province>div>b').next().slideUp(100);
                }
                if (!$('.layerstore').is(":hidden")) {
                    $('.layerstore').hide();
                }
                if (!$('.listsuggest').is(":hidden")) {
                    $('.listsuggest').hide();
                }
                if (!$('.loginwith').is(":hidden")) {
                    $('.loginwith').hide(100);
                }
                if (!$('.intro').is(":hidden")) {
                    $('.intro').hide(100);
                }
                if (!$('#listprovince').is(":hidden")) {
                    $('#listprovince').hide(100);
                }
                if (!$('#listdistrict').is(":hidden")) {
                    $('#listdistrict').hide(100);
                }
                if (!$('li.manu-other .manufacture').is(":hidden")) {
                    $('li.manu-other .manufacture').hide(100);
                }
                if (!$('li.manu-other .price').is(":hidden")) {
                    $('li.manu-other .price').hide(100);
                }
                if (!$('li.manu-other .criteria').is(":hidden")) {
                    $('li.manu-other .criteria').hide(100);
                }
                if (!$('li.sort-filter .sortprice').is(":hidden")) {
                    $('li.sort-filter .sortprice').hide(100);
                }
                if (!$('.criteria .sortprice').is(":hidden")) {
                    $('.criteria .sortprice').hide(100);
                }
                if (!$('div.dropdown ul').is(":hidden")) {
                    $('div.dropdown ul').hide(100);
                }
                if (!$('li.li-feature-tablet .property').is(":hidden")) {
                    $('li.li-feature-tablet .property').hide(100);
                }
                if (!$('.property').is(":hidden")) {
                    $(".criteria").not(this).parent().removeClass('expand');
                    $(".criteria").not(this).next().hide();
                    $(".property").next().slideUp();
                }
                $(".phukien").not(this).parent().removeClass('expand');
                $(".phukien").not(this).next().hide();
                if ($('.phukien').hasClass('actmenu')) {
                    $('.phukien').removeClass('actmenu');
                    $('.phukien').next().slideUp();
                }
                else {
                    $('.phukien').addClass('actmenu');
                    $('.phukien').next().slideDown();
                }
            }

            setTimeout(function () {
                var xhttpCart = new XMLHttpRequest();
                xhttpCart.onreadystatechange = function () {
                    if (this.readyState == 4 && this.status == 200) {
                        var res = JSON.parse(this.responseText);
                        if (res != undefined) {
                            var e1 = document.getElementsByClassName('a1')[0];
                            var cart = document.getElementsByClassName('icontgdd-cart')[0];
                            if (isNewCart2020 == 0) {
                                if (res.quantity != undefined && parseInt(res.quantity) > 0) {
                                    e1.classList.add('actmenu');
                                    cart.innerText = res.quantity;
                                } else {
                                    e1.classList.remove('actmenu');
                                    cart.innerText = '';
                                }
                            }

                            var e2 = document.getElementsByClassName('a2');
                            e2[0].innerText = res.fullname != undefined && res.fullname != '' ? 'Đơn hàng của ' + res.fullname.replace('anh ', 'a.').replace('chị ', 'c.') : 'LỊCH SỬ MUA HÀNG';
                        }
                    }
                };
                xhttpCart.open("GET", "/aj/CommonV3/CartQuantity", true);
                xhttpCart.send();

                UpdateCartQuantity();
            }, 100);
